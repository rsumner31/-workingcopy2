+        float townScale = 1f * width / bWidth;
+        float offsetx = 0;
+        float offsety = height / 2 - bHeight / 2;
+
+//        matrix.postScale(townScale, townScale);
+//        matrix.postTranslate(offsetx, offsety);
+//
+//        canvas.drawBitmap(mSky, matrix, null);
